@section scripts{
    <script src="~/Scripts/lightslider.js"></script>
    <script>
        $(function () {
            $('.laptop, .fon, .tablet, .gif').addClass('opaque');
        });

        $("#content-slider").lightSlider({                
            item: 3,
            auto: true,
            loop: true,
            pauseOnHover: true,
            pause: 4000,
            speed: 800,
            slideMargin: 0,
            controls: false,
            enableDrag: false,
            freeMove: false,
            pager: false,

            onBeforeStart: function (el) {
                $('.prvi').addClass('zumIn');
            },
            onBeforeSlide: function (el) {
                $('.active').next().removeClass('zumIn');
                if ($('.active').next().next().hasClass('prvi')) {
                    $('.prvi').addClass('zumIn');
                }
                $('.active').next().next().addClass('zumIn');
            },
            onAfterSlide: function (el) {
                if ($('.active').hasClass('prvi')) {
                    $('.prvi').removeClass('zumIn');
                }                    
            }
        });

        $('#sp1').delay(100).animate({ left: 150 }, 1200).delay(500).animate({ left: -50 }, 500).animate({ left: 1500 }, 1000, function () {
            var div = document.getElementById("sp1");
            div.parentNode.removeChild(div);

            $('#sp101').animate({ top: 100 }, 700, 'linear').animate({ top: 60 }, 500).delay(100).animate({ top: 100 }, 500, 'linear').animate({ top: 100 }, 1500, function () {
                $(this).addClass('fadeOut').delay(700).queue(function () {

                    $('#sp201').addClass('sp3-f').delay(1500).queue(function () {
                        var divs = document.getElementsByClassName('sp2');
                        for (var i = 0; i < 16; i++) {
                            divs[0].parentNode.removeChild(divs[0]);
                        }
                        $('#sp202').addClass('sp4-f').delay(1500).queue(function () {
                            $('.sp3, .sp4').addClass('transparent');
                            $('.logo').delay(800).queue(function () {
                                $(this).addClass('opaque');
                            });
                        });
                    });
                });
            });
            $('#sp102').delay(50).animate({ top: 110 }, 700, 'linear').animate({ top: 60 }, 500).delay(100).animate({ top: 100 }, 500, 'linear').animate({ top: 100 }, 1400, function () {
                $(this).addClass('fadeOut');
            });
            $('#sp103').delay(100).animate({ top: 110 }, 700, 'linear').animate({ top: 60 }, 500).delay(100).animate({ top: 100 }, 500, 'linear').animate({ top: 100 }, 1300, function () {
                $(this).addClass('fadeOut');
            });
            $('#sp104').delay(150).animate({ top: 110 }, 700, 'linear').animate({ top: 60 }, 500).delay(100).animate({ top: 100 }, 500, 'linear').animate({ top: 100 }, 1200, function () {
