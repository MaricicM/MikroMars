                $(this).addClass('fadeOut');
            });
            $('#sp107').delay(300).animate({ top: 110 }, 700, 'linear').animate({ top: 60 }, 500).delay(100).animate({ top: 100 }, 500, 'linear').animate({ top: 100 }, 900, function () {
                $(this).addClass('fadeOut');
            });
            $('#sp108').delay(350).animate({ top: 110 }, 700, 'linear').animate({ top: 60 }, 500).delay(100).animate({ top: 100 }, 500, 'linear').animate({ top: 100 }, 800, function () {
